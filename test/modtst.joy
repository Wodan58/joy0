DEFINE
  zero == 0 0 pop;
  one == zero 1 +;
  HIDE
    un == one;
    deux == zero one un + +
  IN
    two == deux;
    three == zero un + deux +
  END;
  four == two three + one -
END

HIDE
  trois == two succ;
  HIDE
    eins == one;
    zwei == one dup +;
    drei == eins zwei +
  IN
    quatre == drei eins +
  END;
  cinq == two trois +
IN
  five == cinq;
  six == trois three +;
  seven == trois drei + succ
END

HIDE
  twenty == 20;
  thirty == 30
IN
  fifty == twenty thirty +
END

0 __settracegc.
zero.
one.
two.
three.
four.
five.
six.
seven.
fifty.
